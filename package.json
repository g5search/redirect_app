{
  "name": "redirect_app",
  "version": "1.1.0",
  "description": "A redirect service for incoming secure domain requests using DNS-validated SSLs",
  "main": "server.js",
  "scripts": {
    "test": "NODE_ENV=test jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:watch": "jest --watch --coverage",
    "start": "NODE_ENV=production node ./server.js",
    "dev": "NODE_ENV=development nodemon ./server.js",
    "sql:start": "./cloud_sql_proxy -instances=domain-forward-218421:us-central1:domain-forwarder-a=tcp:5432",
    "deploy": "./deploy.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/g5search/redirect_app.git"
  },
  "author": "Tyler Hasenoehrl",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/g5search/redirect_app/issues"
  },
  "engines": {
    "node": "12.22.12",
    "npm": "6.14.15"
  },
  "homepage": "https://github.com/g5search/redirect_app#readme",
  "dependencies": {
    "@google-cloud/logging-winston": "^5.3.0",
    "@greenlock/manager": "^3.1.0",
    "@root/greenlock": "^4.0.5",
    "@root/greenlock-express": "^4.0.4",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "pg": "^7.12.1",
    "sequelize": "^6.32.0",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "greenlock-manager-test": "^3.1.1",
    "jest": "^26.6.1",
    "nodemon": "^2.0.22",
    "supertest": "^3.4.2"
  }
}
