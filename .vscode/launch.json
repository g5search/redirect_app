 "version": "0.2.0",
 "configurations": [

  {
   "type": "node",
   "name": "vscode-jest-tests",
   "request": "launch",
   "args": [
    "--runInBand"
   ],
   "cwd": "${workspaceFolder}",
   "console": "integratedTerminal",
   "internalConsoleOptions": "neverOpen",
   "program": "${workspaceFolder}/node_modules/jest/bin/jest"
  },
  {
   "type": "node",
   "request": "launch",
   "name": "No Greelock",
   "program": "${workspaceFolder}/app/lib/index.js"
  },
  {
   "type": "node",
   "request": "launch",
   "name": "Manager Test",
   "program": "${workspaceFolder}/manager.test.js"
  }
 ]
}